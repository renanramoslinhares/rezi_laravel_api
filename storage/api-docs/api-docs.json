{
    "openapi": "3.0.0",
    "info": {
        "title": "API Rezi",
        "version": "1.0"
    },
    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "operationId": "loginUser",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "renanramoslinhares@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "API_rezi123"
                                    }
                                },
                                "required": [
                                    "email",
                                    "password"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login bem-sucedido",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Login bem-sucedido"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "4|KoWRT2D85S42fe0zjT9CaWswM8nHHihrKNxglr2j8d2514b7"
                                        },
                                        "user": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "Renan"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "renanramoslinhares@gmail.com"
                                                },
                                                "email_verified_at": {
                                                    "type": "string",
                                                    "nullable": true,
                                                    "example": null
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-11-04T04:12:09.000000Z"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2024-11-04T14:28:57.000000Z"
                                                }
                                            },
                                            "required": [
                                                "id",
                                                "name",
                                                "email",
                                                "created_at",
                                                "updated_at"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciais inválidas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Credenciais inválidas"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/pages": {
            "get": {
                "tags": [
                    "Pages"
                ],
                "operationId": "getPages",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de páginas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 5
                                            },
                                            "title": {
                                                "type": "string",
                                                "example": "About Us"
                                            },
                                            "content": {
                                                "type": "string",
                                                "example": "<h1>About Us</h1><p>Welcome to our website.</p>"
                                            },
                                            "status": {
                                                "type": "string",
                                                "example": "published"
                                            },
                                            "user_id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "example": "2024-11-04T13:55:29.000000Z"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "example": "2024-11-04T13:55:29.000000Z"
                                            }
                                        },
                                        "required": [
                                            "id",
                                            "title",
                                            "content",
                                            "status",
                                            "user_id",
                                            "created_at",
                                            "updated_at"
                                        ]
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Pages"
                ],
                "operationId": "createPage",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Título da Página"
                                    },
                                    "content": {
                                        "type": "string",
                                        "example": "<p>Conteúdo da página em HTML.</p>"
                                    },
                                    "status": {
                                        "type": "string",
                                        "example": "draft"
                                    }
                                },
                                "required": [
                                    "title",
                                    "content",
                                    "status"
                                ]
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Página criada com sucesso",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "message": {
                                                "type": "string",
                                                "example": "Página criada com sucesso"
                                            },
                                            "page": {
                                                "type": "object",
                                                "properties": {
                                                    "title": {
                                                        "type": "string",
                                                        "example": "Título da Página .xX"
                                                    },
                                                    "content": {
                                                        "type": "string",
                                                        "example": "<p>Conteúdo da página em HTML.</p>"
                                                    },
                                                    "status": {
                                                        "type": "string",
                                                        "example": "draft"
                                                    },
                                                    "user_id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-04T15:39:03.000000Z"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-04T15:39:03.000000Z"
                                                    },
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 13
                                                    }
                                                },
                                                "required": [
                                                    "title",
                                                    "content",
                                                    "status",
                                                    "user_id",
                                                    "updated_at",
                                                    "created_at",
                                                    "id"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "401": {
                            "description": "Não autorizado",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "message": {
                                                "type": "string",
                                                "example": "Não autorizado"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users": {
            "post": {
                "tags": [
                    "Users"
                ],
                "operationId": "createUser",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Seu Nome"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "email@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "sua_senha"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "example": "sua_senha"
                                    }
                                },
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ]
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Usuário criado com sucesso",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "message": {
                                                "type": "string",
                                                "example": "Usuário criado com sucesso"
                                            },
                                            "user": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Seu Nome"
                                                    },
                                                    "email": {
                                                        "type": "string",
                                                        "example": "email@example.com"
                                                    },
                                                    "email_verified_at": {
                                                        "type": "string",
                                                        "nullable": true,
                                                        "example": null
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-04T04:12:09.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-04T14:28:57.000000Z"
                                                    }
                                                },
                                                "required": [
                                                    "id",
                                                    "name",
                                                    "email",
                                                    "created_at",
                                                    "updated_at"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "400": {
                            "description": "Erro ao criar usuário",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "message": {
                                                "type": "string",
                                                "example": "Erro ao criar usuário"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user": {
            "get": {
                "tags": [
                    "Users"
                ],
                "operationId": "getUser",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuário recuperado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Renan"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "renanramoslinhares@gmail.com"
                                        },
                                        "email_verified_at": {
                                            "type": "string",
                                            "nullable": true,
                                            "example": null
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-11-04T04:12:09.000000Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2024-11-04T14:28:57.000000Z"
                                        }
                                    },
                                    "required": [
                                        "id",
                                        "name",
                                        "email",
                                        "created_at",
                                        "updated_at"
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Não autorizado"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}